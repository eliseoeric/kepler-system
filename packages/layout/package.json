{
  "name": "@eliseoeric/layout",
  "version": "0.1.2",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo node_modules dist",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@eliseoeric/eslint-config": "workspace:*",
    "@eliseoeric/tokens": "workspace:*",
    "@eliseoeric/typescript-config": "workspace:*",
    "@storybook/test": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@types/node": "^22.15.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "eslint": "^9.33.0",
    "tsup": "^8.0.2",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "@eliseoeric/primitives": "workspace:*"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
