{
  "name": "@repo/layout",
  "version": "0.1.0",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "exports": {
    "./*": "./dist/*.js"
  },
  "scripts": {
    "build": "tsup",
    "watch": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo node_modules dist",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/tokens": "workspace:*",
    "@repo/core": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "eslint": "^9.33.0",
    "tsup": "^8.0.2",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "access": "public"
  }
}
